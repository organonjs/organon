{
  "name": "conventional-changelog-organon",
  "version": "0.0.1",
  "description": "A wrapper to use emojis with conventional-changelog and commitlint",
  "keywords": [
    "commitlint",
    "config",
    "conventional",
    "changelog"
  ],
  "license": "MIT",
  "type": "commonjs",
  "main": "cjs/index.js",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib",
    "!lib/**/*.map",
    "cjs",
    "!cjs/**/*.map"
  ],
  "author": {
    "name": "Jason Lenoble",
    "email": "jason.lenoble@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/organonjs/organon/tree/main/tooling/conventional-changelog"
  },
  "scripts": {
    "build": "heft test --clean"
  },
  "dependencies": {
    "@types/conventional-changelog-core": "~4.2.1",
    "conventional-changelog-conventionalcommits": "~5.0.0"
  },
  "devDependencies": {
    "@organon/commitlint-config": "workspace:*",
    "@organon/emoji-presentation-map": "workspace:*",
    "@organon/organon-node-rig": "workspace:*",
    "@rushstack/eslint-config": "~2.6.1",
    "@rushstack/heft": "~0.45.8",
    "@rushstack/heft-jest-plugin": "~0.3.8",
    "@types/heft-jest": "~1.0.3",
    "eslint": "~8.18.0"
  }
}
